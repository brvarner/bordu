<div id="<%= dom_id task %>" class="card">
  <div class="card-body">
  <a href="/tasks/<%= task.id %>">
  <p>
    <strong>Project:</strong>
    <%= task.project_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= task.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= task.description %>
  </p>

  <p>
    <strong>Creator:</strong>
    <%= task.creator_id %>
  </p>

  <p>
    <strong>Completed at:</strong>
    <%= task.completed_at %>
  </p>
  </a>
  </div>
</div>
